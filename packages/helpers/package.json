{
  "name": "@relay-protocol/helpers",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@bgd-labs/aave-address-book": "4.8.0",
    "ethers": "6.13.5"
  },
  "scripts": {
    "start": "tsc --watch",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint",
    "build:ts": "tsup src/index.ts --dts --format esm,cjs",
    "build": "yarn build:ts && copyfiles -f src/abis dist"
  },
  "devDependencies": {
    "@relay-protocol/networks": "workspace:^",
    "@relay-protocol/tsconfig": "workspace:^",
    "@types/copyfiles": "^2",
    "copyfiles": "2.4.1",
    "eslint": "9.17.0",
    "tsup": "8.3.5",
    "typescript": "5.7.2"
  }
}
