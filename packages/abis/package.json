{
  "name": "@relay-protocol/abis",
  "version": "1.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "release": "npm publish",
    "clean": "rm -rf src/abis dist",
    "build:contracts": "yarn workspace @relay-protocol/smart-contracts build",
    "build:export-abis": "yarn workspace @relay-protocol/smart-contracts build:abis",
    "build:index-from-versions": "sh ./bin/ref-versions.sh",
    "build:ts": "tsup src/index.ts --dts --format esm,cjs",
    "build:copy-versions": "mkdir -p dist/versions && cp -r src/versions/* dist/versions/",
    "build": "yarn build:contracts && yarn build:export-abis && yarn build:index-from-versions && yarn build:ts && yarn build:copy-versions",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint",
    "snapshot": "./bin/snapshot.sh"
  },
  "devDependencies": {
    "@relay-protocol/helpers": "^workspace:*",
    "@relay-protocol/networks": "1.1.1",
    "tsup": "8.4.0",
    "typescript": "5.8.3"
  },
  "files": [
    "abis",
    "versions",
    "index.js",
    "dist"
  ]
}