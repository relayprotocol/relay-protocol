{
  "name": "@relay-protocol/abis",
  "version": "0.0.2",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "abis",
    "index.js"
  ],
  "scripts": {
    "release": "npm publish",
    "clean": "rm -rf src dist",
    "build:export-abis": "yarn workspace @relay-protocol/smart-contracts build",
    "build:ts": "tsup src/index.ts --dts --format esm,cjs",
    "build": "yarn build:export-abis && yarn build:ts"
  },
  "devDependencies": {
    "@relay-protocol/helpers": "^workspace:*",
    "@relay-protocol/networks": "^workspace:*",
    "tsup": "8.3.6",
    "typescript": "5.7.3"
  }
}
